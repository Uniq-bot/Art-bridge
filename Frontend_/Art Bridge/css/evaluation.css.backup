@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&display=swap');
*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Poppins', sans-serif;
}

body::selection{
    background-color: #000000;
    color: white;
}

#page {
    width: 100%;
    min-height: 100vh;
    display: flex;
    flex-direction: column;
    overflow-x: hidden;
}

/* Main styles */
main {
    width: 100%;
    position: relative;
    flex: 1;
    display: flex;
    flex-direction: column;
}

/* Hero Section Styles */
#heroSection {
    width: 100%;
    min-height: 100vh;
    position: relative;
    background-image: url('https://images.unsplash.com/photo-1605719125065-3dd9e3f79057?q=80&w=1170&auto=format&fit=crop&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');
    background-size: cover;
    background-position: center;
    display: flex;
    align-items: flex-end;
    padding: 100px 5%;
}

#heroSection::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(
        to bottom,
        rgba(0, 0, 0, 0.1) 0%,
        rgba(0, 0, 0, 0.4) 50%,
        rgba(0, 0, 0, 0.8) 100%
    );
    z-index: 1;
}

#heroSection .hero-content {
    position: relative;
    color: white;
    z-index: 2;
    max-width: 800px;
    margin-bottom: 5%;
}

#heroSection h1 {
    font-size: 4.5rem;
    font-weight: 700;
    margin-bottom: 1.5rem;
    line-height: 1.1;
}

#heroSection h1 span {
    color: #9c8ac7;
    display: block;
}

#heroSection p {
    font-size: 1.25rem;
    line-height: 1.6;
    margin-bottom: 2rem;
    max-width: 700px;
}

/* About Section */
.about-section, .Services-section, .cove-section, .Evaluation-section {
    padding: 100px 5%;
    max-width: 1200px;
    margin: 0 auto;
    width: 100%;
}

.about-header h1, .services-header h1, .evaluation-header h1 {
    font-size: 2.5rem;
    margin-bottom: 2rem;
    color: #2c3e50;
}

.about-content, .services-content1, .evaluation-content {
    font-size: 1.1rem;
    line-height: 1.8;
    color: #333;
    margin-bottom: 2rem;
}

.services-content1 ul {
    list-style-type: none;
    margin: 1.5rem 0;
}

.services-content1 li {
    margin-bottom: 1rem;
    padding-left: 1.5rem;
    position: relative;
}

.services-content1 li:before {
    content: 'â€¢';
    color: #9c8ac7;
    font-size: 1.5rem;
    position: absolute;
    left: 0;
    top: -2px;
}

.services-content2, .cove-content {
    background-color: #f9f9f9;
    padding: 2rem;
    border-radius: 8px;
    margin: 2rem 0;
}

/* COVES Section */
.cove-section {
    background-color: #f5f5f5;
}

.cove-container {
    display: flex;
    gap: 3rem;
    align-items: center;
}

.cove-text {
    flex: 1;
}

.cove-img {
    flex: 1;
    border-radius: 8px;
    overflow: hidden;
    box-shadow: 0 10px 30px rgba(0,0,0,0.1);
}

.cove-img img {
    width: 100%;
    height: auto;
    display: block;
}

/* Partner Stories */
.partner-section {
    padding: 100px 5%;
    background-color: #fff;
}

.partner.header {
    text-align: center;
    margin-bottom: 3rem;
}

.partner-flex {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 2rem;
    max-width: 1400px;
    margin: 0 auto;
}

.story-item {
    background: #fff;
    border-radius: 8px;
    overflow: hidden;
    box-shadow: 0 5px 15px rgba(0,0,0,0.1);
    transition: transform 0.3s ease;
}

.story-item:hover {
    transform: translateY(-5px);
}

.story-item img {
    width: 100%;
    height: 200px;
    object-fit: cover;
}

.story-item > p {
    padding: 1rem 1.5rem 0;
    color: #9c8ac7;
    font-weight: 600;
}

.story-item h3 {
    padding: 0.5rem 1.5rem;
    color: #2c3e50;
}

.story-item p em {
    display: block;
    padding: 0 1.5rem 1.5rem;
    font-style: normal;
    color: #666;
    line-height: 1.6;
}

/* Newsletter Section */
.last {
    background-color: #7965B2;
    color: white;
    padding: 80px 5%;
    text-align: center;
}

.last h1 {
    font-size: 2.5rem;
    margin-bottom: 1rem;
}

.last p {
    font-size: 1.1rem;
    margin-bottom: 2rem;
    max-width: 600px;
    margin-left: auto;
    margin-right: auto;
}

.ib {
    display: flex;
    justify-content: center;
    gap: 1rem;
    max-width: 600px;
    margin: 0 auto;
}

.ib input[type="email"] {
    flex: 1;
    padding: 1rem;
    border: none;
    border-radius: 4px;
    font-size: 1rem;
}

.ib button.sign {
    background-color: #fff;
    color: #7965B2;
    border: none;
    padding: 0 2rem;
    border-radius: 4px;
    font-weight: 600;
    cursor: pointer;
    transition: background-color 0.3s ease;
}

.ib button.sign:hover {
    background-color: #f0f0f0;
}

/* Responsive Design */
@media (max-width: 1024px) {
    .cove-container {
        flex-direction: column;
    }
    
    .cove-img {
        width: 100%;
        margin-top: 2rem;
    }
}

@media (max-width: 768px) {
    #heroSection h1 {
        font-size: 3rem;
    }
    
    .about-section, .Services-section, .cove-section, .Evaluation-section, .partner-section {
        padding: 60px 5%;
    }
    
    .ib {
        flex-direction: column;
    }
    
    .ib input[type="email"], .ib button.sign {
        width: 100%;
    }
}

@media (max-width: 480px) {
    #heroSection h1 {
        font-size: 2.2rem;
    }
    
    .about-header h1, .services-header h1, .evaluation-header h1 {
        font-size: 2rem;
    }
}

#heroSection .hero-content h1 {
    font-size: 4rem;
    font-weight: 600;
    margin-bottom: 1rem;
    line-height: 1.2;
}

#heroSection .hero-content h1 span {
    font-style: italic;
}

#heroSection .hero-content p {
    font-size: 1.2rem;
